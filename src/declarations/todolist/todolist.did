type Todo = 
 record {
   content: text;
   createTime: Time;
   id: text;
   priority: nat;
 };
type Time = int;
service : {
  Delete: (text) -> ();
  addTodo: (text, nat) -> ();
  complete: (text) -> ();
  delComplete: (text) -> ();
  getCompleteList: () -> (vec Todo) query;
  getCompleteListSize: () -> (nat) query;
  getTodoList: () -> (vec Todo) query;
  getTodoListSize: () -> (nat) query;
}
